let ชื่อตัวแปร;                  
    let age;
let ชื่อตัวแปร = ค่าเริ่มต้น;        
    age = 19;
let ชื่อตัวแปร = ค่าเริ่มต้น,ชื่อตัวแปร = ค่าเริ่มต้น   
    let a,b,c,d;
    let x = 1, y = 2, z = 3;


(ค่าคงที่ไม่สามารถเปลี่ยนค่าได้)
const ค่าคงที่
const ชื่อตัวแปร = ค่าของตัวแปร;
const age = 20;
age = 50; //เปลี่ยนค่าเดิมไม่ได้ ค่าเดิมคือ 20